                        </div>
                    </div>
                    <div class="property-type-module medium-module">
                        <div class="login_input">
                            <br />
                            <label>
                                Upload Logo</label>
                            <asp:FileUpload ID="UpdLogo" runat="server" />
                            <asp:Image ID="imgLogo" style="max-height:120px;max-width:300px;"  runat="server" Visible="false" />
                        </div>
                    </div>
                    <div class="property-type-module medium-module">
                        <div class="login_input">
                            <br />
                            <label>Favicon.ico</label>
                                
                            <asp:FileUpload ID="UpdFavicon" runat="server" />
                            <asp:Image ID="imgfavicon" Width="32px" Height="32px" runat="server" Visible="false" />
                        </div>
                    </div>  
                       <div class="property-type-module medium-module">
                        <div class="login_input">
                            <br />
                            <label>
                               Header Image</label>
                            <asp:FileUpload ID="updbanner" runat="server" />
                            <asp:Image ID="Imgbanner" style="max-height:120px;max-width:300px;" runat="server" Visible="false" />
                        </div>
                    </div>
                            <div class="property-type-module medium-module">
                        <div class="login_input">
                            <br />
                            <label>
                              Upload Banners</label>
                            <asp:FileUpload ID="FileUpload1" MaxLength="3"  runat="server" class="multi" />
                            <asp:Label ID="Label1" runat="server"></asp:Label>
                            <asp:Image ID="Image1" Width="100px" Height="100px" runat="server" Visible="false" />
                        </div>
                    </div>
                      <div class="property-type-module medium-module">
                        <div class="login_input">
                            <br />
                            <label>
                                Site Template</label>
                            <asp:TextBox ID="txtSiteTemplate" runat="server" PlaceHolder="Site Template"></asp:TextBox>
                             <%--<ajaxtoolkit:FilteredTextBoxExtender ID="FilteredTextBoxExtender2" runat="server" TargetControlID="txtSiteTemplate" FilterMode="ValidChars" FilterType="LowercaseLetters,UppercaseLetters,custom" ValidChars="@,_" ></ajaxtoolkit:FilteredTextBoxExtender>--%>
                        </div>
                    </div>
                    <div class="property-type-module medium-module">
                        <div class="login_input">
                            <label>
                                Banner Settings</label>
                            <asp:TextBox ID="txtBanner" runat="server" PlaceHolder="Banner Settings"></asp:TextBox>
                            <%--<ajaxtoolkit:FilteredTextBoxExtender ID="FilteredTextBoxExtender3" runat="server" TargetControlID="txtBanner" FilterMode="ValidChars" FilterType="LowercaseLetters,UppercaseLetters,custom" ValidChars="@,_" ></ajaxtoolkit:FilteredTextBoxExtender>--%>
                        </div>
                        <div class="login_inputs" style="float:left;">
                            <asp:Label ID="lblerror" runat="server" ForeColor="Red"></asp:Label>
                            <asp:Button ID="btnSave"  runat="server" Text="Save" onclick="btnSave_Click" />
                             <asp:Button ID="btnBack" style="clear:none;" runat="server" Text="Back" onclick="btnBack_Click"  />
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</asp:Content>
