.nav {
    list-style: none;
     *zoom: 1;
    
}
.nav:before,
.nav:after {
    content: " "; 
    display: table; 
}
.nav:after {
    clear: both;
}
.nav ul {
    list-style: none;
    width: 9em;
}
.nav a {
    color: #818285;
   font-family:'TrajanPro-Regular';
    font-size: 13px;
    padding: 6px 19px;
    text-decoration: none;
	text-transform:uppercase;
}
.nav li {
    position: relative;
}
.nav > li {
     background: rgba(0, 0, 0, 0) url("../images/menu-strip.png") no-repeat scroll 10px 14px;
    float: left;
    padding: 5px;
    width:50%;
}
.nav > li > .parent {
    background-image: url("../images/downArrow.png");
    background-repeat: no-repeat;
    background-position: right;
}
.nav > li > a {
    display: block;
}

.nav li ul {
    left: -9999px;
     margin: 0 0 0 -63px;
    padding: 0;
    position: absolute;
    width: 272px;
    z-index:999999999;
}
.nav > li.hover > ul {
    left: 0;
}
.nav li li.hover ul {
    left: 100%;
    top: 0;
}
.nav li li a {
    background: #231f20 none repeat scroll 0 0;
    border: 1px solid black;
    display: block;
    font-family: arial;
    font-size: 13px;
    position: relative;
    z-index: 100;
	color:white;
}
.nav li li li a {
    background:#249578;
    z-index:200;
    border-top: 1px solid red;
}

